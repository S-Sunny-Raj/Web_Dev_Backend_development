<%- include('./partials/header') -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="form-group">
    <label for="tText">Title</label>
    <input class="form-control" type="text" name="tText" />
    <label for="pText">Post</label>
    <textarea
      class="form-control"
      name="pText"
      id="pText"
      cols="30"
      rows="5"
    ></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include('./partials/footer') -%>
